void setup() { //no need to declare pinMode when you use analogPin
}

void loop() {
  analogWrite(11,0);
  delay(500);
  analogWrite(11,50);
  delay(500);
  analogWrite(11,100);
  delay(500);
  analogWrite(11,150);
  delay(500);
  analogWrite(11,200);
  delay(500);
  analogWrite(11,255);
  delay(500);
  //Lighting up an LED gradually
}
